<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {% block title %}
            <title>Frascolino</title>
            <meta name="description" content="TBD" />
            <meta property="og:title"              content="Frascolino" />
            <meta property="og:description"        content="STBD" />
            <meta property="og:image"              content="{{ absolute_url( asset('assets/static/SEO.jpg') ) }}" />
        {% endblock %}
        <meta property="og:url"                content="{{ app.request.getSchemeAndHttpHost ~ app.request.baseUrl ~ app.request.pathInfo }}" />


        {#
        <link rel="stylesheet" href="https://kit.fontawesome.com/27ed148731.css" crossorigin="anonymous">
        <link rel="stylesheet" href="https://unpkg.com/swiper@11.0.6/swiper-bundle.min.css">
        <script src="https://unpkg.com/swiper@11.0.6/swiper-bundle.min.js"></script>
        #}

        <script src="https://cdn.suerte.studio/gsap/gsap.min.js"></script>
        <script src="https://cdn.suerte.studio/pixi.js/pixi.min.js"></script>


        <link rel="stylesheet" href="{{ asset('assets/style_app.css?v=' ~ appversion() ~ cv()) }}">
        <link rel="stylesheet" href="{{ asset('assets/style_mobile.css?v=' ~ appversion() ~ cv()) }}">


        <link rel="icon" type="image/png" href="{{ asset('assets/static/favicon-48x48.png') }}" sizes="48x48" />
        <link rel="icon" type="image/svg+xml" href="{{ asset('assets/static/favicon.svg') }}" />
        <link rel="shortcut icon" href="{{ asset('assets/static/favicon.ico') }}" />
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('assets/static/apple-touch-icon.png') }}" />
        <meta name="apple-mobile-web-app-title" content="Frascolino" />
        <link rel="manifest" href="{{ asset('assets/static/site.webmanifest') }}" />

        <link rel="stylesheet" href="https://cdn.suerte.studio/misc/cookieconsent.css">


        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}



        <!-- Google tag (gtag.js) --> <script async src="https://www.googletagmanager.com/gtag/js?id=G-DB7EFKQGX4"></script> <script> window.dataLayer = window.dataLayer || []; function gtag(){dataLayer.push(arguments);} gtag('js', new Date()); gtag('config', 'G-DB7EFKQGX4'); </script>


    </head>
    <body id="main-content">


        {%  include 'app/nav-app.html.twig' %}



        <main id="swup" class="transition-main">

            <div class="loader">

            </div>

            {% block body %}{% endblock %}

            {% set route = app.request.get('_route') %}
            {% set fex = false %}
            {% if route in ['organizza_evento'] %}
                {% set fex = true %}
            {% endif %}

            {{ include('app/footer-app.html.twig', {expand: fex}) }}
        </main>


        <script src="https://cdn.suerte.studio/jquery/jquery.min.js"></script>
        <script src="https://cdn.suerte.studio/misc/imagesloaded.pkgd.min.js"></script>
        <script src="https://cdn.suerte.studio/swup/Swup.umd.js"></script>
        <script src="https://cdn.suerte.studio/swup/head-plugin.umd.js"></script>
        <script src="https://cdn.suerte.studio/swup/slide-plugin.umd.js"></script>
        <script src="https://cdn.suerte.studio/swup/slide-theme.umd.js"></script>


        <script src="{{ asset('assets/app.js?v=' ~ appversion() ~ cv()) }}"></script>
        <script type="module" src="{{ asset('assets/cookieconsent-config.js') }}"></script>


        {% block bottom_js %}
        {% endblock %}
    </body>
</html>
